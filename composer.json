{
    "name": "kartavik/php-comparison",
    "type": "library",
    "description": "A library to represent equivalence relations and strategies for hashing and sorting values.",
    "keywords": [
        "phpcommon",
        "comparison",
        "equivalence",
        "hash",
        "sort"
    ],
    "license": "MIT",
    "homepage": "https://github.com/marcospassos/phpcommon-comparison",
    "authors": [
        {
            "name": "Marcos Passos",
            "email": "marcos@marcospassos.com"
        }
    ],
    "require": {
        "php": "^5.5 | ^7.0",
        "kartavik/php-intmath": "^1.0",
        "symfony/polyfill-php70": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4 | ^6.0 | ^7.0 | ^8.2",
        "php-mock/php-mock-phpunit": "^1.1",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "^2.3 | ^3.2",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpCommon\\Comparison\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCommon\\Comparison\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox"
    }
}
