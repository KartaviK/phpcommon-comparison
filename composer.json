{
  "name": "phpcommon/comparison",
  "type": "library",
  "description": "A library to represent equivalence relations and strategies for hashing and sorting values.",
  "keywords": ["phpcommon", "comparison", "equivalence", "hash", "sort"],
  "license": "MIT",
  "homepage": "https://github.com/marcospassos/phpcommon-comparison",
  "authors": [
    {
      "name": "Marcos Passos",
      "email": "marcos@marcospassos.com"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "phpcommon/intmath": "~1.0@dev"
  },
  "require-dev": {
    "php": "^5.6|^7.0",
    "phpunit/phpunit": "~5.4",
    "php-mock/php-mock-phpunit": "^1.1",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "PhpCommon\\Comparison\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpCommon\\Comparison\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --testdox"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
